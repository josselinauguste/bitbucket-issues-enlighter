{
  "name": "Bitbucket issues enlighter",
  "version": "1.0.0",
  "description": "Make bitbucket's issues page a little more easy to read",
  "manifest_version": 2,
  "content_scripts": [
      {
          "matches": [ "*://bitbucket.org/*/issues*" ],
          "css": [ "bitbucket-issues-enlighter.css" ],
          "js": [ "bitbucket-issues-enlighter.js" ],
          "run_at": "document_end"
      }
  ],
  "web_accessible_resources": ["bitbucket-issues-enlighter.css", "bitbucket-issues-enlighter.js"]
}
